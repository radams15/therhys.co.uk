% layout 'page', title => 'Links';

<div class="title centre">
    <h1 class="centre">Links</h1>
</div>

<div class="about centre_page">
    <ul>
        <% for(my $i=0 ; $i<scalar(@$links) ; $i+=2) { %>
            <li><a href='<%= $links->[$i+1] %>'> <%= $links->[$i] %> </a></li>
        <% } %>
    </ul>
</div>
